<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.srpay.in">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_24"
        android:label="@string/app_name"
        android:name=".MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".UI.Splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".UI.login.LoginActivity"/>
        <activity android:name=".UI.SideNavigation.NavigationDrawer"/>
        <activity android:name=".UI.Recharge.RechargeActivity"/>
        <activity android:name=".UI.DTH.DTHActivity"/>
        <activity android:name=".UI.NetworkOperator.OperatorActivity"/>
    </application>

</manifest>